
{% load template_utils %}
<VideoMessage xmlns="http://indivo.org/vocab/xml/documents#">
      
  <fileId>{{ fobj.fileId }}</fileId>
  

  <storageType>{{ fobj.storageType }}</storageType>
  

  <subject>{{ fobj.subject }}</subject>
  

  <from>{{ fobj.from }}</from>
  

  <dateRecorded>{{ fobj.dateRecorded|format_iso8601_datetime }}</dateRecorded>
  

  <dateSent>{{ fobj.dateSent|format_iso8601_datetime }}</dateSent>
  

</VideoMessage>
  